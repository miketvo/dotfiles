{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#5D677A",
          "foreground": "transparent",
          "style": "diamond",
          "leading_diamond": "\u250c\ue0b2",
          "properties": {
            "always_enabled": true
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "background": "#5D677A",
          "foreground": "#FFFFFF",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "{{.FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#61AFEF",
          "foreground": "transparent",
          "invert_powerline": true,
          "style": "diamond",
          "template": " {{ .Name }} ",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "always_enabled": true
          },
          "type": "shell"
        },
        {
          "background": "#56B6C2",
          "foreground": "transparent",
          "style": "powerline",
          "template": " # {{ .UserName }}@{{ .HostName }} ",
          "powerline_symbol": "\ue0b0",
          "type": "session"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#5D677A",
          "style": "plain",
          "template": "\u251c\u2500[",
          "type": "text"
        },
        {
          "foreground": "#61AFEF",
          "properties": {
            "folder_separator_icon": "\\",
            "style": "full"
          },
          "style": "plain",
          "template": " <b>{{ .Path }}</b> ",
          "type": "path"
        },
        {
          "foreground": "#5D677A",
          "style": "plain",
          "template": "] ",
          "type": "text"
        },
        {
          "style": "plain",
          "foreground": "#5D677A",
          "template": "on <#FFFFFF>git:</><#E5C07B>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}</>{{ end }}{{ if .Working.Changed }} <#FFFFFF>\u2190</> <#E06C75>\uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#FFFFFF>\u2190</> <#98C379>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <#FFFFFF>\u2190</> <#C678DD>\uf51e {{ .StashCount }}</>{{ end }}",
          "properties": {
            "branch_max_length": 25,
            "fetch_status": true,
            "fetch_stash_count": true,
            "branch_icon": "\ue0a0"
          },
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#5D677A",
          "style": "plain",
          "template": "\u2514\u2500\u25b6 ",
          "type": "text"
        },
        {
          "foreground": "#E5C07B",
          "style": "plain",
          "template": "\uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#C678DD",
          "style": "plain",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#E06C75",
          "foreground": "transparent",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0 ",
          "template": " \uf071 {{ .Meaning }} ",
          "type": "exit"
        },
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "<b>$</b>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
