{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "background": "#96525C",
            "foreground": "transparent",
            "style": "diamond",
            "leading_diamond": "\u250c\ue0b2",
            "properties": {
              "always_enabled": true
            },
            "template": " {{ .CurrentDate | date .Format }} ",
            "type": "time"
          },
          {
            "background": "#96525C",
            "foreground": "#FFFFFF",
            "properties": {
              "always_enabled": true
            },
            "style": "plain",
            "template": "{{.FormattedMs }} ",
            "type": "executiontime"
          },
          {
            "background": "#D37C96",
            "foreground": "transparent",
            "invert_powe96525Crline": true,
            "style": "diamond",
            "template": " {{ .Name }} ",
            "trailing_diamond": "\ue0b0",
            "properties": {
              "always_enabled": true
            },
            "type": "shell"
          },
          {
            "background": "#8ABEB7",
            "foreground": "transparent",
            "style": "powerline",
            "template": " # {{ .UserName }}@{{ .HostName }} ",
            "powerline_symbol": "\ue0b0",
            "type": "session"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#96525C",
            "style": "plain",
            "template": "\u251c\u2500[",
            "type": "text"
          },
          {
            "foreground": "#D37C96",
            "properties": {
              "folder_separator_icon": "/",
              "style": "full"
            },
            "style": "plain",
            "template": " <b>{{ .Path }}</b> ",
            "type": "path"
          },
          {
            "foreground": "#96525C",
            "style": "plain",
            "template": "] ",
            "type": "text"
          },
          {
            "style": "plain",
            "foreground": "#96525C",
            "template": "on <#FFFFFF>git:</><#E0DE48>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}</>{{ end }}{{ if .Working.Changed }} <#FFFFFF>\u2190</> <#AC79BB>\uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#FFFFFF>\u2190</> <#B5D680>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <#FFFFFF>\u2190</> <#8ABEB7>\uf51e {{ .StashCount }}</>{{ end }}",
            "properties": {
              "branch_max_length": 25,
              "fetch_status": true,
              "fetch_stash_count": true,
              "branch_icon": "\ue0a0"
            },
            "type": "git"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#96525C",
            "style": "plain",
            "template": "\u2514\u2500\u25b6 ",
            "type": "text"
          },
          {
            "foreground": "#FFFD76",
            "style": "plain",
            "template": "\uf0e7 ",
            "type": "root"
          },
          {
            "foreground": "#AC79BB",
            "style": "plain",
            "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
            "type": "python"
          },
          {
            "background": "#BD0940",
            "foreground": "transparent",
            "style": "diamond",
            "leading_diamond": "\ue0b2",
            "trailing_diamond": "\ue0b0 ",
            "template": " \uf071 {{ .Meaning }} ",
            "type": "exit"
          },
          {
            "foreground": "#BD0940",
            "style": "plain",
            "template": "<b>$</b>",
            "type": "text"
          }
        ],
        "type": "prompt"
      }
    ],
    "final_space": true,
    "version": 2
  }
